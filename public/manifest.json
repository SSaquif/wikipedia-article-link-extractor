{
  "manifest_version": 3,
  "name": "Wikipedia Article Link Extractor",
  "version": "1.0",
  "description": "Extract all internal Wikipedia article links from a page.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/wiki/*"],
      "js": ["assets/content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
